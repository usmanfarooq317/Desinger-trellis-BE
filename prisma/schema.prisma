generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            Int      @id @default(autoincrement())
  firstName     String
  lastName      String
  firmName      String
  email         String   @unique
  contact       String
  numOfEmployees Int
  password      String
  createdAt     DateTime @default(now())
  updatedAt     DateTimeÂ @updatedAt
}